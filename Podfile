platform :ios, '8.0'
use_frameworks!


def podsToInstall
    inherit! :search_paths
    pod 'SideMenu', '~>2.1.5'
    pod 'Charts', '~>3.0.1'
end

target 'Booster Kiwisaver' do
    podsToInstall
end

target 'Booster KiwisaverTests' do
    podsToInstall
end

target 'Booster KiwisaverUITests' do
    podsToInstall
end
