platform :ios, '8.0'
use_frameworks!


def podsToInstall
    inherit! :search_paths
    pod 'SideMenu'
    pod 'Charts'
end

target 'Booster Kiwisaver' do
    podsToInstall
end

target 'Booster KiwisaverTests' do
    podsToInstall
end

target 'Booster KiwisaverUITests' do
    podsToInstall
end
